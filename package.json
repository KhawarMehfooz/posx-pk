  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "wait-on": "^9.0.3"
  }
